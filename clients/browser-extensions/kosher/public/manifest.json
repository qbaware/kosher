{
  "name": "Kosher",
  "version": "0.0.1",
  "author": "Qbaware",
  "description": "Synchronize and use tabs from all of your browsers into a single unified storage.",
  "manifest_version": 2,
  "browser_action": {
      "default_icon": "./resources/logo.png",
      "default_popup": "./index.html"
  },
  "background": {
      "scripts": [
          "./scripts/background.js"
      ]
  },
  "permissions": [
      "tabs",
      "alarms"
  ]
}